<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.blackpinguin.android.sindwirschonda"
    android:versionCode="45"
    android:versionName="0.7" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="de.blackpinguin.android.sindwirschonda.activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="de.blackpinguin.android.sindwirschonda.activities.CalcTimeActivity"
            android:label="@string/calcTime" >
        </activity>
        <activity
            android:name="de.blackpinguin.android.sindwirschonda.activities.CalcDistanceActivity"
            android:label="@string/calcDistance" >
        </activity>
        <activity
            android:name="de.blackpinguin.android.sindwirschonda.activities.CalcSpeedActivity"
            android:label="@string/calcSpeed" >
        </activity>
    </application>

</manifest>
