<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="SIValue">
        <attr name="label" format="string" />
        <attr name="value" format="float" />
        <attr name="unit" format="enum">
            <enum name="time" value="1" />
            <enum name="distance" value="2" />
            <enum name="speed" value="3" />
        </attr>    
        <attr name="unitIndex" format="integer" />
        <attr name="backgroundColor" format="color" />
    </declare-styleable>
    
</resources>
